{
  "name": "create-hibana",
  "version": "0.1.6",
  "description": "Scaffold a Cloudflare Workers + Ruby project from the official template.",
  "license": "MIT",
  "type": "module",
  "bin": {
    "create-hibana": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "keywords": [
    "cloudflare",
    "workers",
    "ruby",
    "cli",
    "scaffolding"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hiroeorz/hibana.git"
  },
  "bugs": {
    "url": "https://github.com/hiroeorz/hibana/issues"
  },
  "homepage": "https://github.com/hiroeorz/hibana#readme",
  "author": "Hibana Contributors",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "typecheck": "tsc --noEmit",
    "link:global": "npm link",
    "link:runtime": "npm link @hibana-apps/runtime",
    "unlink:runtime": "npm unlink --no-save @hibana-apps/runtime || true"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "commander": "^12.1.0",
    "degit": "^2.8.4",
    "kleur": "^4.1.5"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "tsup": "^8.0.1",
    "typescript": "^5.4.5",
    "vitest": "^1.6.1"
  }
}
